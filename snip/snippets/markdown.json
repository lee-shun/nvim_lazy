{
  "env": {
    "prefix": "env",
    "body": ["\\begin{$1}", "\t$0", "\\end{$1}"],
    "description": "environment"
  },
  "matrix": {
    "prefix": "matrix",
    "body": ["\\left[", "\\begin{matrix}", "\t$1", "\\end{matrix}", "\\right]"],
    "description": "matrix environment"
  },
  "aligned": {
    "prefix": "aligned",
    "body": [
      "\\left\\{",
      "\\begin{aligned}",
      "\t$1",
      "\\end{aligned}",
      "\\right."
    ],
    "description": "aligned environment"
  },
  "frac": {
    "prefix": "frac",
    "body": ["\\frac{$1}{$2}$0"],
    "description": "frac"
  }
}
